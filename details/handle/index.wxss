

.swiper-tab {
  padding: 0 40rpx;
  display: flex;
}

.total{
  font-size: 15rpx;
  background-color: #FF9000;
  border-radius: 50%;
  position: absolute;
  width: 35rpx;
  height: 35rpx;
  line-height: 35rpx;
  top: 25rpx;
  /* z-index: 999; */
}

.swiper-tab-item {
  height: 66rpx;
  width: 33.3%;
  font-size: 30rpx;
  background: #8bc76a;
  color: #fff;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  text-align: center;
  line-height: 66rpx;
}

.swiper-tab-item:nth-child(1) {
  border-radius: 10rpx 0rpx 0rpx 10rpx;
}
.generate{
  position: fixed;
  top:66rpx;
  right:10rpx;
  width:200rpx;
  height:100rpx;
  background-color: red;
}
.printingNow{
  position: absolute;
  top:-81rpx;
  right:10rpx;
  width:174rpx;
  height:70rpx;
  background-color: #ff9000;
  text-align: center;
  line-height: 70rpx;
  color: #fff;
  border-radius: 10rpx;
}
.swiper-tab-item:nth-child(3) {
  border-radius: 0rpx 10rpx 10rpx 0rpx;
}

