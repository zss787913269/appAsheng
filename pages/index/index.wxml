<view wx:if="{{tableListone!=''}}" style="padding-bottom:180rpx;height:auto;">

	<view class="main">
		<view class="left">

			<scroll-view scroll-y="true" style="height: 1100rpx" scroll-into-view="true" scroll-with-animation="true">

				<view wx:for="{{tableListone}}" wx:if="{{item.goods !=''}}" wx:key="unique">

					<view class="{{classfiySelect == item.id ? 'selected':'normal'}}" data-id='{{item.id}}'>
						<text>{{item.name}}</text>
					</view>


				</view>

			</scroll-view>

		</view>

		<view class='right'>
			<view wx:for="{{tableListone}}" wx:if="{{item.goods !=''}}" wx:key="unique">

				<view class="title">
			<text>{{item.name}}</text>
			<text bindtap="toOldFood" data-value="{{item.name}}" wx:if="{{groupStart}}">新菜推荐: >></text>
		</view>
				<scroll-view scroll-y="true" bindscroll="scroll" scroll-top="{{scrollTop}}" scroll-into-view="{{'inToview'+rigId}}" scroll-with-animation="true">

					<!-- <view class='itemTitle' id="{{'inToview'+item.id}}">{{item.title}} -->

						<block wx:for="{{item.goods}}" wx:for-list="item">

							<!-- <view class='itemTitle' id="{{'inToview'+item.id}}">{{item.title}} -->

							<view class="mukaui" bindtap="clickse" data-id="{{item}}" data-index="{{index}}" wx:if="{{item.is_adv == 0}}">
								<view class="cainame">
									<text>{{item.brand_name}}</text>
								</view>
								<view class="caimony">
									<text class="caimonyf9">￥{{item.price}}元/{{item.inventory_unit}} </text>
									<view>
										<view class="textColor" wx:for="{{item.show_keyword}}" wx:key="index" wx:for-item="itemName" wx:for-index="indexS">
											{{indexS}}:{{itemName}}</view>
									</view>
									<text id="book">
                <text id="book-text" >订</text>
									</text>
								</view>
							</view>

						</block>
			
				</scroll-view>
			</view>
		</view>

	</view>

</view>




