							<view class="start">
								<view class="startPrice shopColor text-wei">组团开始价
									<text class="donwn"> </text>
								</view>
								<view class="shopSpecifications">{{item.spec[0].title}}:{{item.spec[0].value[0]}}</view>
							</view>
							<view class="sprice">
								<text class="startWei">￥</text>
								<text class="initialPrice">{{item.price}}</text>
								<text class="txtCol-group startWei">/{{item.inventory_unit}}</text>
							</view>
						</view>
						<view class="grouostart-left">
							<view class="start">
								<view class="startPrice shopColor text-wei PriceRise">每隔{{item.raise_time}}分钟涨价{{item.raise_price}}
									<text class="donwn"> </text>
								</view>
								<view class="shopSpecifications">{{item.spec[0].title}}:{{item.spec[0].value[0]}}</view>
							</view>

							<view class="sprice">
								<text class="initialPrice nowPrice">当前价格</text>
								<text class="startWei">￥</text>
								<text class="initialPrice">{{item.nowPrice}}</text>
								<text class="txtCol-group startWei">/{{item.inventory_unit}}</text>
							</view>
						</view>
					</view>
					<view class="group-bottom">
						<view class="group-bottom-left">
							<text class="shopTitle groupLeft {{item.color_type == 1?'shopColor':''}} {{item.color_type == 2?'shopColor2':''}} {{item.color_type == 3?'shopColor3':''}}">{{item.title}}</text>
							<text class="minimum groupLeft">最低订购量{{item.goods_number}}</text>
						</view>
						<view class="group-bottom-left ">
							<text class="shopTitle groupLeft">总库存：{{item.total_inventory}}</text>
							<text class="minimum groupLeft">剩：{{item.inventory}}</text>
						</view>
					</view>
					<view class="purchase-group" wx:if="{{item.purchase}}">
						<view class="liang-group">
							<text class="liantext-group float">数量 </text>
							<view class="zhonged-group background-group float" bindtap="bindPlusGroup">
								<text> +</text>
							</view>
							<view class="zhong-group corlor-group float">
								<input type="number" value="{{numGroup}}" disabled="true" />
							</view>
							<view class="zhonged-group borde-group float" bindtap="bindMinusGroup">
								<text>-</text>
							</view>
						</view>
						<view class="numGroup-num">￥{{item.nowPrice * 100 * numGroup / 100}}</view>
						<view class="purchase-right">
							<view class="purchase-btn" bindtap="personalPurchase">个人采购</view>
							<view class="purchase-btn" bindtap="hotelPurchase" data-price="{{item.nowPrice}}">酒店采购</view>
						</view>

					</view>
				</view>
			</view>
