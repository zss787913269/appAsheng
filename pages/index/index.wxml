					</block>


				</view>
			</scroll-view>
			<view class="cover_screen" bindtap="hideBuyModal" wx:if="{{showModalStatus}}"></view>
			<view animation="{{animationData}}" class="buy_box" wx:if="{{showModalStatus}}">
				<view class="termine shopright" bindtap="clickcancel">
					<text>关闭</text>
				</view>
				<view style='color:red;font-size:34rpx'>{{mylist.title}}</view>
				<view class="shopPrice">￥{{shopPrice*1000*num/1000}}元</view>

				<view class="gg">
					<view class="one" wx:for="{{tableid}}" wx:key="key">
						<text class="jt">{{item.title}}</text>
						<view class="onebox">
							<view style="margin-top:20rpx" wx:for="{{item.value}}" wx:key="unique" data-index="{{indexName}}" data-indx="{{item.title}}" bindtap="clickgui" wx:for-item="itemName" wx:for-index="indexName">
								<text class="{{itemName == true ? 'itemActive':'item'}}"  >{{indexName}} </text>
							</view>
						</view>

					</view>
				</view>
        <view class="bz">
          <text>备注 ： </text>
          <input placeholder="请输入备注" bindinput="modalInput" data-name="inputBz"></input>
        </view>


				<view class="liang">
					<text class="mycount" >数量 </text>
					<view class="zhonged borde background add" bindtap="bindPlus">
						<text class="myadd"> +</text>
					</view>
					<view class="zhong corlor  myinput">
						<input type="number" bindchange="bindManual" value="{{num}}" />
					</view>
					<view class="zhonged borde " bindtap="bindMinus">
						<text class="myadd">-</text>
					</view>
				</view>

				<view class="mybottom">

