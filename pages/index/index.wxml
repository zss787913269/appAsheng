<view class="mimg">
  <swiper autoplay="true">
    <swiper-item wx:for="{{ads}}" wx:key="key">
      <image src="{{item.images_url}}" mode="widthFix" />
    </swiper-item>
  </swiper>
</view>
<c style="height:25rpx;"></c>
<view class="div_div">
  <!-- <view class="advertisement" bindtap="lookAdvertisement">
    <image src="{{advertisement}}" class="advertisement-img"></image>
  </view> -->
  <swiper class="swiper-one" current="{{currentIndex}}">
    <swiper-item wx:for="{{pageNumber}}" wx:key="index" wx:for-index="indexName">

      <view wx:if="{{index<(indexName+1)*8 && indexName*8<=index}}" wx:for="{{tableList}}" wx:key="unique" class="{{firstclass==item.id?'boxshow':''}} bannerIm" bindtap="goUrl" data-url="{{item}}">
				<!-- <text class="mycarcount"  >{{item.count}}</text> -->
        <image wx:if="{{item.icon!=''}}" src="{{item.icon}}"  class="imagesss" />
        <image wx:else src="/images/index/rk_6.jpg" style="height:66rpx;" class="imagesss" />
        <view>{{item.name}}
					<text  wx:if="{{item.count}}"  class="count">{{item.count}}</text>
				</view>
				
				<view></view>
      </view>
    </swiper-item>
  </swiper>

  <c style="height:25rpx;"></c>
</view>


	
<view class="scroll_table"  id="main" bindtap="myscorll">
  <scroll-view scroll-x>
    <view class="swiper-tab">
      <view wx:for="{{tableListre}}" wx:key="unique" class="swiper-tab-item {{currentTab==item.id?'active':''}}" data-current="{{item.id}}" bindtap="clickTab">{{item.name}}

			<text class="mycarcount" wx:if="{{item.count}}">{{item.count}}</text>
		
      </view>
    </view>
  </scroll-view>

</view>

