<view class="top">
	<view class="header">
		<i class="fal fa-search"></i>
		<navigator url="/details/search/index?where=shop" hover-class="other-navigator-hover">
			<input placeholder="请输入商品名">
			</input>
		</navigator>
	</view>

</view>
<view>
	<swiper autoplay="true">
		<swiper-item wx:for="{{ads}}" wx:key="key">
			<image src="{{item.images_url}}" mode="widthFix" />
		</swiper-item>
	</swiper>
</view>
<c style="height:25rpx;"></c>
<view class="div_div">
	<view class="advertisement" bindtap="lookAdvertisement">
		<image src="{{advertisement}}" class="advertisement-img"></image>
	</view>
	<swiper class="swiper-one" current="{{currentIndex}}">
		<swiper-item wx:for="{{pageNumber}}" wx:key="index" wx:for-index="indexName">
			<view wx:if="{{index<(indexName+1)*6 && indexName*6<=index}}" wx:for="{{tableList}}" wx:key="unique" class="{{firstclass==item.id?'boxshow':''}} bannerIm" bindtap="goUrl" data-url="{{item}}">
				<image wx:if="{{item.icon!=''}}" src="{{item.icon}}" style="height:66rpx;" class="imagesss" />
				<image wx:else src="/images/index/rk_6.jpg" style="height:66rpx;" class="imagesss" />
				<text>{{item.name}} </text>
			</view>
		</swiper-item>
	</swiper>

	<c style="height:25rpx;"></c>
</view>
<view class="scroll_table">
	<scroll-view scroll-x>
		<view class="swiper-tab">
			<view wx:for="{{tableListre}}" wx:key="unique" class="swiper-tab-item {{currentTab==item.id?'active':''}}" data-current="{{item.id}}" bindtap="clickTab">{{item.name}}

			</view>
		</view>
	</scroll-view>

</view>
<view class="back" wx:if="{{back == true}}" bindtap="backTop">
	<!-- <i class="fal fa"></i> -->
	<i class="fal fa-arrow-circle-up" style="font-size:60rpx;color:#fff;background-color:#62a241;border-radius:50%;"></i>
</view>
