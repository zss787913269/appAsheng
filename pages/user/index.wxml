<view class="top">
  <view class="tbody">
    <view class="user-header-avatar">
      <open-data class="" lang="zh_CN" type="userAvatarUrl"></open-data>
    </view>
    <view class="user-header-name">
      <open-data type="userNickName"></open-data>
      <view class="daxiao">
        可提现积分
      </view>
    </view>
    <view class="chongzhi">
      <navigator url="/person/recharge/index" hover-class="other-navigator-hover">
        <text>充值</text>
      </navigator>
    </view>
    <image src="/images/index/home_top_01.jpg" mode="widthFix"></image>
    <view class="card" style="background: url('data:image/jpg;base64,');background-size:contain;">
      <view class="head_tap" data-url="{{url}}" bindtap="gotoikPage">
        <text>{{commission}}</text>
        <text>我的佣金</text>
      </view>
      <view class="head_tap" data-url="{{url1}}" bindtap="gotoikPageS">
        <text>{{integral}}</text>
        <text>我的积分</text>
      </view>

      <view class="head_tap" data-url="{{url2}}" bindtap="gotoikPage">
        <text>{{balance}}</text>
        <text>我的余额</text>
      </view>
    </view>
    <!-- <image src="/images/index/home_top_03.jpg" mode="widthFix"></image> -->
  </view>
</view>
