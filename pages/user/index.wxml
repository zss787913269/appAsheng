<!--操作菜单-->

<view class="view_power">

  <!-- <view class="user_tab">
   
  </view> -->
  <c style="height:20rpx;background:#eee;"></c>
  <view class="user_tab">
    <view bindtap='privateKitchen'>
      <text class="fal {{optionList2.clas}}" style="color:{{optionList2.color}}"></text>{{optionList2.name}}
      <i class="fal fa-angle-right"></i>
    </view>
  </view>
  <view class="user_tab">
    <view wx:for="{{optionList}}" wx:key="unique" data-url='{{item.url}}' bindtap='getOption' data-index="{{index}}">
      <text class="fal {{item.clas}}" style="color:{{item.color}}"></text>{{item.name}}
      <sup wx:if="{{99>=pre && pre !==0 && index === 0}}" class="total">{{pre}}</sup>
      <sup wx:elif="{{pre>99&& index === 0}}" class="total">99+</sup>
      <sup wx:if="{{99>=reply && reply !==0 && index === 7}}" class="total">{{reply}}</sup>
      <sup wx:elif="{{reply>99&& index === 7}}" class="total">99+</sup>
      <i class="fal fa-angle-right"></i>
    </view>
  </view>
</view>
<view class="become" wx:if="{{modality}}">
  <view class="become-top">
    <image src="/images/details/cook.png" mode="widthFix" class="become-img"></image>
    <view class="become-cook">成为私厨</view>
  </view>
  <view class="become-bottom">
    <view class="btn determine" bindtap="determine">确定</view>
    <view class="btn cancel" bindtap="cancel">取消</view>
  </view>
</view>