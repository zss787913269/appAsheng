<view class="top">

	<view class="top-top">
		<view class="imgbox" bindtap="sq">
    <view wx:if="{{show}}" class="myav">
      <view class="avtor">
          <open-data lang="zh_CN" type="userAvatarUrl"></open-data>
        </view>
        <view class="name">
          <open-data type="userNickName"></open-data>
        </view>
    </view>

    <view wx:else class="myav">
      <view class="avtor">
          <image  src="http://second.chchgg.com/static/index/default/images/logo.jpg"></image>
        </view>
        <view class="name">
          <text>点击登录账号</text>
        </view>
    </view>

		</view>
	</view>

	<view class="top-bottom">
		<view class="item">
			<text class="price">0</text>
			<text class="text">佣金</text>
		</view>
		<view class="item">
			<text  class="price">{{integral}}</text>
			<text class="text">积分</text>
		</view>
		<view class="item" bindtap="golyj">
			<text  class="price">{{pledge}}</text>
			<text class="text">履约金</text>
		</view>
		<view class="item2" bindtap="gotoikPage" data-url="{{url2}}">
			<text  class="price">{{balance}}</text>
			<text class="text">余额(元)</text>
		</view>

	</view>


	<view class="top-right">可提现积分</view>



</view>

