  right: 33rpx;
  top: 30rpx;
  color: #868686;
  font-size: 30rpx;


}
.timebox{
  margin-left: 48rpx;
  margin-top: 23rpx;
  display: flex;
  justify-content: space-between;

}
.time{
  font-size:22rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(134,134,134,1);
}
.ok{
  margin-right: 23rpx;
  font-size:26rpx;
  font-family:Source Han Sans CN;
  font-weight:400;
  color:rgba(134,134,134,1);
  opacity:1;
}
.all{
  display: flex;
