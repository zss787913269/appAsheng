  flex-direction: column;
  align-items: center;
}
.top-title{
  text-align: center;
  margin-top: 58rpx;
}
.top-title text{
  
  font-size: 40rpx;
  font-family: Source Han Sans CN;
  font-weight: 500;
  color: #000000;
}
