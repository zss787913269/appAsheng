.card{
width: 100%;
height: 550rpx;
padding: 20rpx;
box-sizing: border-box;
}
.myflex{
  margin-top: 200rpx;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
.myflex text{
  font-size: 30rpx;
  font-family: Source Han Sans CN;
  font-weight: 400;
}
.noTime{
  width:100%;
  height:100ropx;
  text-align: center;
  line-height: 100rpx;
}
.cardname{
  width: 100%;
  height: 100%;
  box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.1);
  padding: 30rpx 0;
  text-align: center;
  box-sizing: border-box;
  position: relative;
}
.releaseFood{
  position: absolute;
  top:33rpx;
  right:45rpx;
}
.cardname image{
  width: 155rpx;
  height: 155rpx; 
  margin-bottom:15rpx; 
}
.xuanjie{
  display: flex;
  flex-flow: column;
}
.fontsiz{
  color: #717171;
  font-size: 25rpx;
  line-height: 50rpx;
}

.cardtime{
width: 100%;
height: 260rpx;
padding: 20rpx;
box-sizing: border-box;
}
.timeyue{
  width: 100%;
  height: 100%;
  box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.1);
  padding: 30rpx 20rpx;
  box-sizing: border-box;
}
.timeyue view{
  margin-bottom: 20rpx;
}
.timeyue view:nth-child(2){
  display: flex;
}
.color{
    color: #ff9000
}
.backgr{
  width: 150rpx;
  background: #eee;
  margin-left: 20rpx;
  padding: 0rpx 20rpx;
  border-radius:5rpx; 
}
.list{
  width: 95%;
  margin: 0 auto;
  box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.1);
  margin-bottom:30rpx; 
}
.title{
  height: 100rpx;
  width: 100%;
  padding-left:20rpx;
  padding-right:20rpx;
  line-height: 100rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #ccc;
}
/* 预约列表 */
.middle{
border-bottom:1px solid #ccc;
padding-bottom: 20rpx;
box-sizing: border-box;  
}
.tbody{
  height: 150rpx;
  width: 100%;
  padding:20rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.portrait{
  display: flex;
}
.introduce{
  display: flex;
  flex-flow: column;
  line-height: 50rpx;
  margin-left:20rpx 
}
.portrait image{
  width: 100rpx;
  height: 100rpx;

}
.accept{
  width: 150rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius:30rpx;
  background:#ff9000;
  color: #fff; 
  margin-top: 25rpx; 
}
.accept1{
  width: 150rpx;
  height: 50rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius:30rpx;
  background:#ccc;
  color: #fff; 
  margin-top: 25rpx; 
}

.nottom{
   padding-left:20rpx;
  padding-right:20rpx;
  box-sizing: border-box;
}

.redact_btn{
  color: rgb(101, 101, 238);
}

.food_margin{
  margin-left: 20rpx;
}
.top{
  margin-top: 102rpx;
