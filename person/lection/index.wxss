
.xinchuang{
  width: 100%;
  padding: 0 20rpx;
  box-sizing: border-box;  
  
}
.craed{
  height: 200rpx;
  width: 100%;
  margin-top: 20rpx;
  display: flex;
  box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.1);
}
.imged{
  width: 200rpx;
  height: 200rpx; 
  padding: 20rpx;
  box-sizing: border-box;  
}
.craed .imged image{
  width: 155rpx;
  height: 155rpx; 
}
.craed .xuanjie{
  width: 480rpx;
  height:200rpx; 
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding: 20rpx 0;
  box-sizing: border-box;
  
}
.shouc{
  display: flex;
  flex-flow: column;
  line-height: 40rpx;

}
.shouctwo{
   display: flex;
   justify-content: space-between;
}
.shouctwo image{
   width: 30rpx;
   height: 30rpx;
   vertical-align: middle;
   margin-left: 20rpx;
   margin-right: 10rpx;
}

.yuyue{
   width: 150rpx;
  height: 200rpx; 
  display: flex;
  justify-content: center;
  align-items: center;
}
.balck{
  width: 130rpx;
  height: 50rpx; 
  background:#ffcf4c; 
  color: #fff;
  text-align: center;
  line-height: 50rpx;
   border-radius: 10rpx;
}

.shenhe{
  color: #717171;
  font-size: 25rpx
}
.xiang{
  color: #ffcf4c;
}
